{
  "name": "@streammint/client",
  "version": "1.0.0",
  "description": "A TypeScript Client for StreamMint Server",
  "keywords": [
    "TypeScript",
    "Chat",
    "Client",
    "WebSocket",
    "Real-time"
  ],
  "license": "MIT",
  "author": "Satadeep Dasgupta <satadeep.dasgupta@brainiuminfotech.com>",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "build:watch": "tsup --watch",
    "dev": "tsup --watch",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "type-check": "tsc --noEmit",
    "clean": "rimraf dist",
    "prepublishOnly": "npm run clean && npm run build && npm run test",
    "prepack": "npm run build"
  },
  "dependencies": {
    "axios": "^1.9.0",
    "eventemitter3": "^5.0.1",
    "linq-to-typescript": "^12.0.0",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.6",
    "@typescript-eslint/eslint-plugin": "^6.16.0",
    "@typescript-eslint/parser": "^6.16.0",
    "@vitest/coverage-v8": "^3.2.3",
    "@vitest/ui": "^3.2.3",
    "eslint": "^8.56.0",
    "rimraf": "^5.0.5",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "vitest": "^3.2.3"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/streammint.git",
    "directory": "client"
  },
  "bugs": {
    "url": "https://github.com/your-org/streammint/issues"
  },
  "homepage": "https://github.com/your-org/streammint#readme"
}
